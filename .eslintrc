{
  "extends": "eslint-config-airbnb",
  "env": {
    "amd": true
  },
  "globals": {
    "document": "readonly",
    "self": "readonly",
    "monaco": "readonly",
    "_globalIPCRenderer": "readonly",
    "_globalRepoInfo": "readonly",
    "_globalPref": "readonly",
    "_globalPathModule": "readonly",
    "_globalAMDRequire": "readonly",
    "_globalLanguageArr": "readonly",
    "_globalProbList": "writable",
    "_globalDownLoadModeInd": "writable",
    "_globalCodeEditor": "writable",
    "_globalDescEditor": "writable"
  },
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      1,
      {
        "ignore": [
          "electron"
        ]
      }
    ],
    "import/prefer-default-export": "off",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": 0,
    "max-len": [
      1,
      240,
      2,
      {
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "no-console": 0,
    "no-plusplus": [
      1,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_globalIPCRenderer",
          "_globalRepoInfo",
          "_globalLanguageArr",
          "_globalPathModule",
          "_globalAMDRequire",
          "_globalPref",
          "_globalProbList",
          "_globalCodeEditor",
          "_globalDescEditor",
          "_globalDownLoadModeInd"
        ]
      }
    ],
    "no-restricted-globals": ["self"],
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "_global",
        "args": "after-used",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^event"
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true
      }
    ],
    "object-shorthand": 0,
    "quotes": [
      "error",
      "double"
    ]
  }
}